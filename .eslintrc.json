{
  "root": true,
  "env": {
    "node": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["jest"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error"
  }
}
